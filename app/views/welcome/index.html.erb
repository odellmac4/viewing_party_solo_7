
<h1>Viewing Party</h1>

<%if session[:user_id].present?%>
   <h2>Existing Users</h2>
   <ul id="existing_users">
      <% @users.each do |user| %>
         <li><%= user.email%></li>
      <% end %>
   </ul>
   <%=button_to 'Logout', logout_path, method: :delete%>
<%else%>
   <%= link_to 'Log In', login_path %>
   <%= button_to 'Create New User', register_user_path, method: :get, class: "btn btn-lg btn-outline-secondary" %><br>
<%end%>
